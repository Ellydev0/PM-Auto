{
  "name": "pm-auto",
  "version": "1.0.5",
  "description": "A CLI tool to define and install your tech stack presets with one command.",
  "keywords": [
    "pm-auto",
    "yarn-auto",
    "pnpm-auto",
    "auto"
  ],
  "license": "MIT",
  "author": "Elliot Otoijagha",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pm-auto": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
